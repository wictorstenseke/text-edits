{
  "name": "vite-react-fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate:routes && tsc -b && npm run lint && npm run test && vite build",
    "preview": "vite preview",
    "generate:routes": "tsr generate",
    "type-check": "tsr generate && tsc -b --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "ci": "npm run type-check && npm run lint && npm run test",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "check": "npm run ci",
    "check:full": "npm run type-check && npm run lint && npm run test && vite build",
    "format": "prettier . --write",
    "format:check": "prettier . --check ."
  },
